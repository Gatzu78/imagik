<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main">
    <children>
        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <fx:include fx:id="mainMenu" source="MainMenu.fxml" BorderPane.alignment="CENTER" />
                <SplitPane dividerPositions="0.4" style="-fx-background-color: #7F7941; -fx-border-color: black;">
                    <items>
                        <AnchorPane SplitPane.resizableWithParent="true">
                            <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                <children>
                                    <fx:include fx:id="metadata" source="Metadata.fxml" />
                                </children>
                            </VBox>
                        </AnchorPane>
                        <fx:include fx:id="preview" source="Preview.fxml"  />
                    </items>
                </SplitPane>

            </children>
        </VBox>
    </children>
</AnchorPane>
